{
    "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "sourceMap": false,
      "outDir": "./dist",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "strictNullChecks": false,
      "skipLibCheck": true,
      "noImplicitAny": false,
      "paths": {
        "dotenv": [
          "./node_modules/dotenv/lib/main"
        ],
      }
    },
    "include": [
      "./**/*"
    ]
  }